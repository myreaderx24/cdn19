<script>
 function coinData() {
 return {
 data: null,
 isLoading: true,
 hubpage: false,
 currency: 'USD',
 locale: 'en',
 fetchData() {
 this.isLoading = true;
 var limit = (this.hubPage() ? 4 : 5);
 fetch(`https://pts.igms.io/api/v2/coins/latest?limit=${limit}`)
 .then((res) => res.json())
 .then((data) => {
 this.data = data.data;
 this.isLoading = false;
 });
 },
 formatPrice(value) {
 return new Intl.NumberFormat(this.locale + '-' + this.locale.toUpperCase(), {
 style: 'currency',
 currency: this.currency
 }).format(value);
 },
 isPositive(value) {
 return Math.sign(value) === 1;
 },
 getPropertyForQuote(quote, property) {
 const quoteByCurrency = quote[this.currency];
 return quoteByCurrency[property];
 },
 hubPage() {
 if ('') {
 this.hubpage = true;
 return '';
 } else {
 this.hubpage = false;
 return false;
 }
 }
 };
 }
</script>

